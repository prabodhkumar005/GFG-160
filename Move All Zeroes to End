class Solution {
    void pushZerosToEnd(int[] arr) {
        // code here
        int nonZerosPos=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]!=0){
                arr[nonZerosPos]=arr[i];
                nonZerosPos++;
            }
        }
        for(int i=nonZerosPos;i<arr.length;i++){
            arr[i]=0;
        }
        
    }
}
